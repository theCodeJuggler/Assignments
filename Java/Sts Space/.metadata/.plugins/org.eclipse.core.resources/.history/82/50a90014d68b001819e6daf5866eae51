class Rectangle {
	double length;
	double width;
	
	Rectangle(double l, double w)
	{
		length = l;
		width = w;
	}
	
	double area()
	{
		double ar;
		
		ar=length*width;
		
		return ar;
	}
	
	void perimeter()
	{
		double peri;
		
		peri = 2*(length+width);
		
		System.out.print("Perimeter = "+peri);
	}
}
